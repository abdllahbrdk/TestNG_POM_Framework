<!--We've made a task that paralel executes three classes at the same time -->

<suite name="Test Classes Suite">
    <test name="Test Classes" parallel="classes" thread-count="3">
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="Test._01_LoginTest"></class>
            <class name="Test._02_SubscribeFunctionTest"></class>
            <class name="Test._03_EditAccountTest"></class>
        </classes>
    </test>
</suite>